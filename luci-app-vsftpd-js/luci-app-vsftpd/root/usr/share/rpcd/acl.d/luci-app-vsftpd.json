{
    "luci-app-vsftpd": {
        "description": "Grant access to FTP server configuration",
        "read": {
            "uci": ["vsftpd"],
            "file": {
                "/bin/pidof": ["exec"]
            },
            "ubus": {
                "luci": ["getInitList"]
            }
        },
        "write": {
            "uci": ["vsftpd"],
            "ubus": {
                "luci": ["setInitAction"]
            }
        }
    }
}

